
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<br />
<link href="~/Templates/material_able-main/assets/css/font-awesome-n.min.css" rel="stylesheet" />
<link href="~/Templates/material_able-main/assets/css/font-awesome.min.css" rel="stylesheet" />
<div class="row">
    <div class="col-xl-4 col-md-12">
        <div class="card mat-stat-card">
            <div class="card-block">
                <div class="row align-items-center b-b-default">
                    <div class="col-sm-6 b-r-default p-b-20 p-t-20">
                        <div class="row align-items-center text-center">
                            <div class="col-4 p-r-0">
                                <i class="far fa-user text-c-purple f-24"></i>
                            </div>
                            <div class="col-8 p-l-0">
                                <h5>@ViewBag.ResimCizimCount</h5>
                                <p class="text-muted m-b-0">Resim Çizme Öğretmeni</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 p-b-20 p-t-20">
                        <div class="row align-items-center text-center">
                            <div class="col-4 p-r-0">
                                <i class="fas fa-money-bill-alt text-c-green f-24"></i>
                            </div>
                            <div class="col-8 p-l-0">
                                <h5>@ViewBag.AvgPrice</h5>
                                <p class="text-muted m-b-0">Fiyat Ortalaması</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row align-items-center">
                    <div class="col-sm-6 p-b-20 p-t-20 b-r-default">
                        <div class="row align-items-center text-center">
                            <div class="col-4 p-r-0">
                                <i class="far fa-file-alt text-c-red f-24"></i>
                            </div>
                            <div class="col-8 p-l-0">
                                <h5>@ViewBag.TotalService</h5>
                                <p class="text-muted m-b-0">Hizmet Sayısı</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 p-b-20 p-t-20">
                        <div class="row align-items-center text-center">
                            <div class="col-4 p-r-0">
                                <i class="far fa-envelope-open text-c-blue f-24"></i>
                            </div>
                            <div class="col-8 p-l-0">
                                <h5>@ViewBag.LastClassroom</h5>
                                <p class="text-muted m-b-0">Son Açılan Sınıfımız</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xl-4 col-md-12">
        <div class="card mat-stat-card">
            <div class="card-block">
                <div class="row align-items-center b-b-default">
                    <div class="col-sm-6 b-r-default p-b-20 p-t-20">
                        <div class="row align-items-center text-center">
                            <div class="col-4 p-r-0">
                                <i class="fas fa-share-alt text-c-purple f-24"></i>
                            </div>
                            <div class="col-8 p-l-0">
                                <h5>@ViewBag.MaxBranchTeacher</h5>
                                <p class="text-muted m-b-0">En Çok Branşa Ait Öğretmen Sayısı</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 p-b-20 p-t-20">
                        <div class="row align-items-center text-center">
                            <div class="col-4 p-r-0">
                                <i class="fas fa-sitemap text-c-green f-24"></i>
                            </div>
                            <div class="col-8 p-l-0">
                                <h5>@ViewBag.TotalBranch</h5>
                                <p class="text-muted m-b-0">Branş Sayısı</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row align-items-center">
                    <div class="col-sm-6 p-b-20 p-t-20 b-r-default">
                        <div class="row align-items-center text-center">
                            <div class="col-4 p-r-0">
                                <i class="fas fa-signal text-c-red f-24"></i>
                            </div>
                            <div class="col-8 p-l-0">
                                <h5>@ViewBag.MaxBranch</h5>
                                <p class="text-muted m-b-0">En Çok Branş</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 p-b-20 p-t-20">
                        <div class="row align-items-center text-center">
                            <div class="col-4 p-r-0">
                                <i class="fas fa-book text-c-blue f-24"></i>
                            </div>
                            <div class="col-8 p-l-0">
                                <h5>@ViewBag.TotalMessage</h5>
                                <p class="text-muted m-b-0">Toplam Mesajlar</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xl-4 col-md-12">
        <div class="card mat-clr-stat-card text-white green ">
            <div class="card-block">
                <div class="row">
                    <div class="col-3 text-center bg-c-green">
                        <i class="fas fa-star mat-icon f-24"></i>
                    </div>
                    <div class="col-9 cst-cont">
                        <h5>@ViewBag.AdminCount</h5>
                        <p class="m-b-0">Admin Sayısı</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="card mat-clr-stat-card text-white blue">
            <div class="card-block">
                <div class="row">
                    <div class="col-3 text-center bg-c-blue">
                        <i class="fas fa-trophy mat-icon f-24"></i>
                    </div>
                    <div class="col-9 cst-cont">
                        <h5>@ViewBag.RezCount</h5>
                        <p class="m-b-0">Kayıt Sayısı</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Html.Action("_DashboardTeacherPartial")
</div>